<mapper namespace="com.bible.bible_study_back.mapper.QTMapper">

    <!--모든 데이터-->
    <select id="findAll" resultType="com.bible.bible_study_back.dto.QTDto">
        select * from qt;
    </select>

    <!--QT 게시판 생성-->
    <select id="findByPageQT" resultType="com.bible.bible_study_back.dto.QTDto">
        select * from qt
        where showData = "Y"
        order by id desc limit #{count} offset #{offset};
    </select>

